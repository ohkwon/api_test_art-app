<table class="table table-hover">
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Medium</th>
    <th>Source</th>
  </tr>
  <% @arts.each do |art| %>
    <tr>
      <td><%= art["artist"] %></td>
      <td><%= art["title"] %></td>
      <td><%= art["medium"] %></td>
      <td><%= art["source"] %></td>
    </tr>
  <% end %>
</table>